@model Dish

@{
    ViewData["Title"] = "Home Page";
}
    <div class="row">
        <div class="col-6">
        <h1>Welcome to CRUDelicious</h2>
        <p>Check out some recent dishes!</p>
        <br>


        @{
                foreach(var x in @ViewBag.DishList){
                    <p><a href="Home/Dish/@x.DishId"> @x.Name</a> by @x.Chef</p>
                }
            }
        </div>
</div>
<div class="col-4">
    <div class="row">
        <a  asp-action="Add" >Add Dish</a>
    </div>
</div>